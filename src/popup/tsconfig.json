{
  "compilerOptions": {
    "target": "ES5",
    "module": "NodeNext",
    "jsx": "react-jsx", 
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "NodeNext",
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./",
    "paths": {
      "@/*": ["popup.tsx"]
    }
  },
  "files": ["popup.tsx"]
}
